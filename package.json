{
  "name": "project-starter",
  "version": "1.0.0",
  "description": "A starter kit for Javascript projects",
  "scripts": {
    "prestart": "babel-node internals/message.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch",
    "open:src": "babel-node internals/devServer.js",
    "lint": "esw webpack.config.* src internals --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress internals/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jalexander/project-starter.git"
  },
  "keywords": [
    "javascript",
    "starter"
  ],
  "author": "Jim Alexander",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jalexander/project-starter/issues"
  },
  "homepage": "https://github.com/jalexander/project-starter#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.22.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.1.0",
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.12.0",
    "localtunnel": "^1.8.2",
    "mocha": "^3.2.0",
    "nock": "^9.0.13",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.3",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "^0.0.5"
  }
}
